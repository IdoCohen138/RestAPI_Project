<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.application.service.SlackChannel" table="slackchannel" schema="public"
           catalog="postgres">
        <id name="id">
            <column name="id" sql-type="uuid"/>
        </id>
        <property name="webhook">
            <column name="webhook" sql-type="varchar"/>
        </property>
        <property name="channelName">
            <column name="channelname" sql-type="varchar"/>
        </property>
        <property name="status">
            <column name="status" sql-type="enumstatus" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>